<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.notify">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name="app.notify.NotificationService"></service>
        <activity
            android:name="app.notify.UserNotificationsActivity"
            android:label="Notify"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="app.notify.AddNotificationActivity"
            android:label="@string/add_notification_activity_title"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
